{
  "name": "ng-indigit",
  "version": "0.1.0",
  "description": "An angular directive to make input[type='text'] html elements numerical, with support for fully customizable digit grouping and decimals",
  "keywords": [
    "number",
    "decimal",
    "digit grouping",
    "html input tag",
    "angular"
  ],
  "author": {
    "name": "VahidM",
    "email": "devahidm@gmail.com"
  },
  "repository": {
    "url": "https://github.com/vahidm90/ng-indigit.git",
    "type": "git"
  },
  "peerDependencies": {
    "@angular/common": "^13.3.0",
    "@angular/core": "^13.3.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "module": "fesm2015/ng-indigit.mjs",
  "es2020": "fesm2020/ng-indigit.mjs",
  "esm2020": "esm2020/ng-indigit.mjs",
  "fesm2020": "fesm2020/ng-indigit.mjs",
  "fesm2015": "fesm2015/ng-indigit.mjs",
  "typings": "ng-indigit.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./ng-indigit.d.ts",
      "esm2020": "./esm2020/ng-indigit.mjs",
      "es2020": "./fesm2020/ng-indigit.mjs",
      "es2015": "./fesm2015/ng-indigit.mjs",
      "node": "./fesm2015/ng-indigit.mjs",
      "default": "./fesm2020/ng-indigit.mjs"
    }
  },
  "sideEffects": false
}